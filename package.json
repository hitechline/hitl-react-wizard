{
  "name": "@hitechline/react-wizard",
  "version": "2.0.1",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "clean": "rimraf dist typings",
    "prepublishOnly": "yarn build",
    "release": "semantic-release",
    "release:local": "node-env-run --exec \"semantic-release\"",
    "fix": "yarn lint:fix && yarn prettier:fix",
    "lint": "eslint ./ --ext js,jsx,ts,tsx",
    "lint:fix": "eslint ./ --ext js,jsx,ts,tsx --fix",
    "prettier": "prettier --check ./**/*.{js,jsx,ts,tsx}",
    "prettier:fix": "prettier --write ./**/*.{js,jsx,ts,tsx}",
    "build": "yarn build:check && yarn build:types && yarn build:cjs && yarn build:esm",
    "prebuild": "yarn clean",
    "build:check": "tsc --noEmit",
    "build:types": "tsc -p tsconfig.types.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitechline/react-wizard.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "typings"
  ],
  "keywords": [
    "react",
    "steps",
    "wizard"
  ],
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@hitechline/reactools": "^1.1.2"
  },
  "devDependencies": {
    "@hitechline/eslint-config": "2.0.5",
    "@hitechline/semantic-release-config": "1.2.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.3",
    "@semantic-release/npm": "10.0.4",
    "@semantic-release/release-notes-generator": "11.0.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.43.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "node-env-run": "4.0.2",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "semantic-release": "17.4.7",
    "typescript": "5.1.6"
  }
}
