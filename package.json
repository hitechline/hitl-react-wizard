{
  "name": "@hitechline/react-wizard",
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "clean": "rimraf dist typings",
    "prepublishOnly": "yarn build",
    "release": "semantic-release",
    "release:local": "node-env-run --exec \"semantic-release\"",
    "build": "tsc --outDir dist/es && tsc --outDir dist/cjs --module CommonJS",
    "prebuild": "yarn clean",
    "build:watch": "tsc --outDir dist/es -w",
    "fix": "yarn lint:fix && yarn prettier:fix",
    "lint": "eslint ./ --ext js,jsx,ts,tsx",
    "lint:fix": "eslint ./ --ext js,jsx,ts,tsx --fix",
    "prettier": "prettier --check ./**/*.{js,jsx,ts,tsx}",
    "prettier:fix": "prettier --write ./**/*.{js,jsx,ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitechline/react-wizard.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "typings"
  ],
  "keywords": [
    "react",
    "steps",
    "wizard"
  ],
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@hitechline/reactools": "^1.1.0"
  },
  "devDependencies": {
    "@hitechline/eslint-config-web": "1.1.4",
    "@hitechline/semantic-release-config": "1.2.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.10",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "eslint": "7.22.0",
    "node-env-run": "4.0.2",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.2",
    "typescript": "4.1.3"
  }
}
